class Math {
    static PI {
        return 3.14159265358979323846;
    }

    native static sqrt(x);

    native static ceil(x);

    native static floor(x);

    native static round(x);

    static min(x, y) {
        if (x >= y) return y; else return x;
    }

    static max(x, y) {
        if (x >= y) return x; else return y;
    }

    static abs(x) {
        if (x < 0) return -x; else return x;
    }
}
