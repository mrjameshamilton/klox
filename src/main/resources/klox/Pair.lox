class Pair(first, second) {

    get(index) {
        if (index == 0) {
            return this.first;
        } else if (index == 1) {
            return this.second;
        } else {
            return nil;
        }
    }

    toString() {
        return "Pair(" + this.first + ", " + this.second + ")";
    }
}
