var file = File("src/test/resources/data/text.txt");

print file; // expect: src/test/resources/data/text.txt

var inputStream = FileInputStream(file);
var result;
var str = "";
while (result = inputStream.readChar()) {
    var (c, error) = result;
    if (error is Error) {
        print error.message;
        break;
    }

    if (c == "\n") {
        print str;
        str = "";
    } else {
        str = str + c;
    }
}
inputStream.close();

// expect: line 1
// expect: line 2
// expect: line 3
// expect: line 4
