class Breakfast {
    cook() {
        print "Eggs a-fryin'!";
    }

    serve(who) {
        print "Enjoy your breakfast, " + who + ".";
    }
}

var breakfast = Breakfast();
breakfast.cook(); // expect: Eggs a-fryin'!
breakfast.serve("James"); // expect: Enjoy your breakfast, James.
breakfast.serve("Dave"); // expect: Enjoy your breakfast, Dave.
